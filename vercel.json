{
  "version": 2,
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    },
    {
      "src": "experiments/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/experiments/(.*)", "dest": "/experiments/$1" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ]
}
